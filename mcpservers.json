{
  "mcpServers": {
    "package-docs": {
      "args": [
        "-y",
        "mcp-package-docs"
      ],
      "env": {
        "ENABLE_LSP": "true"
      },
      "command": "npx"
    },
    "github": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "$MCP_GITHUB_PERSONAL_ACCESS_TOKEN"
      },
      "disabled": true,
      "command": "npx"
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    },
    "brave-search": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": "$BRAVE_API_KEY"
      },
      "command": "npx"
    },
    "sequential-thinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx"
    },
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/qorcialwolf"
      ],
      "command": "npx"
    }
  }
}
