{
  "mcpServers": {
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/qorcialwolf"
      ],
      "command": "npx"
    },
    "browser-tools": {
      "args": ["-y", "@agentdeskai/browser-tools-mcp@1.2.0"],
      "command": "npx"
    },
    "package-docs": {
      "command": "npx",
      "args": ["-y", "mcp-package-docs"],
      "env": {
        "ENABLE_LSP": "true"
      }
    },
    "fetch": {
      "args": ["mcp-server-fetch"],
      "command": "uvx"
    },
    "sequential-thinking": {
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "command": "npx"
    },
    "git": {
      "args": ["mcp-server-git", "--repository", "$GIT_ROOT"],
      "command": "uvx"
    },
    "github": {
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": ""
      },
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "mcp/github"
      ],
      "command": "docker"
    },
    "brave-search": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-brave-search"],
      "env": {
        "BRAVE_API_KEY": "$BRAVE_API_KEY"
      }
    }
  }
}
