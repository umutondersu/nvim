{
  "mcpServers": {
    "browser-tools": {
      "args": ["-y", "@agentdeskai/browser-tools-mcp@1.2.0"],
      "command": "npx"
    },
    "sequential-thinking": {
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "command": "npx"
    },
    "github": {
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "mcp/github"
      ],
      "command": "docker",
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": null
      }
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    }
  }
}
