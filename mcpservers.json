{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/qorcialwolf"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "brave-search": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": "$BRAVE_API_KEY"
      },
      "command": "npx"
    },
    "package-docs": {
      "args": [
        "-y",
        "mcp-package-docs"
      ],
      "env": {
        "ENABLE_LSP": "true"
      },
      "command": "npx"
    },
    "github": {
      "disabled": true,
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "$GITHUB_PERSONAL_ACCESS_TOKEN"
      },
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "command": "npx"
    },
    "git": {
      "args": [
        "--directory",
        "$PWD",
        "run",
        "mcp-server-git"
      ],
      "command": "uv",
      "disabled": true
    }
  }
}